{
  "factor": {
    "dbName": "dfs://dayFactorFut",
    "tbName": "pt",
    "dateCol": "tradeDate",
    "symbolCol": "symbol",
    "indicatorCol": "factor",
    "valueCol": "value",
    "condition": "split(symbol,'.')[0] in ['CF','SR','TA','FG','RM','SM','SF','MA','CY','AP','CJ','UR','SA','PF','PK','SH','PX','A','M','C','B','Y','P','V','JD','FB','PP','CS','EG','RR','EB','PG','LH','LU','FU','RB','WR','BU','HC','SP','SS','AO','BR','SI','LC']"
  },
  "label": {
    "dbName": "dfs://dayLabel",
    "tbName": "futureLabel",
    "dateCol": "tradeDate",
    "labelDateCol": "minDate",
    "symbolCol": "cont",
    "indicatorCol": "label",
    "valueCol": "value",
    "condition": "split(cont, '.')[0] in ['CF','SR','TA','FG','RM','SM','SF','MA','CY','AP','CJ','UR','SA','PF','PK','SH','PX','A','M','C','B','Y','P','V','JD','FB','PP','CS','EG','RR','EB','PG','LH','LU','FU','RB','WR','BU','HC','SP','SS','AO','BR','SI','LC']"
  },
  "result": {
    "dbName": "dfs://factorEva",
    "regTbName": "regRes",
    "quaTbName": "quaRes"
  },
  "config": {
    "startDate": "20210101",
    "endDate": null,
    "dailyFreq": true,
    "callBackPeriod": 1,
    "returnIntervals": [1, 3, 5, 10, 20],   // 分层回测调仓周期K = IC法未来K日收益率
    "quantile": 5,
    "dailyPnlLimit": 0.1,
    "useMinFreqPeriod": false,
    "barRetLabelName": "barRet", // 这里不能和下面的futRetLabelNames里面重名
    "futRetLabelNames": ["ret1D","ret3D","ret5D","ret10D","ret20D"]  // 需要和上面returnIntervals的顺序一致
  }
}